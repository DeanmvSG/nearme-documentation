<script src="https://rawgit.com/mdyd-dev/marketplaces/master/volte/dist/js/volte.js" async></script>
<script>
  function showCustomAttributes(attribute){
    $('[data-visibility-dependent]').hide().attr('disabled', true)
    if(attribute.data('show-field') == 'value-dependent'){
      $('[data-visibility-dependent="' + attribute.val().toLowerCase() + '"').show().attr('disabled', false)
    }
  }
  $('[data-show-field]').on('change', function(){ showCustomAttributes($(this))});

  $('[data-show-field]').each(function(){showCustomAttributes($(this));});
</script>
