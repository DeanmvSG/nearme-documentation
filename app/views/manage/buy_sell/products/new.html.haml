- title t("manage.buy_sell.products.new_title")
- render_content_outside_container!
- show_dashboard_navigation

.span10.page-content
  .dash-page
    .dash-head.clearfix
      %a{:href => "#"} MANAGE LISTINGS >
      %p LIST NEW ITEM
      %a.dash-head-btn{:href => ""}
        .btn.blue Bulk Upload
    .dash-body
      = simple_form_for @product_form, wrapper: :product, :url => manage_buy_sell_products_path(@product_form.product), :html => { multipart: true, method: :post, class: 'no_inheritance' } do |f|
        = render 'forms/product_form', f: f