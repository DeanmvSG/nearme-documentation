%tr
  %td= resource.email
  %td= resource.created_instance?
  %td= resource.instance.try(:name)
  %td= link_to 'Edit', edit_admin_instance_creator_path(resource), class: 'btn'
  %td= link_to link_to "Delete", [:admin, resource], :method => :delete, :confirm => "Are you sure you want to delete this?", class: 'btn btn-danger'
