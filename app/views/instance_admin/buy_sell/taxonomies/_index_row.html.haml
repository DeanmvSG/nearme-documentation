%tr
  %td= link_to taxonomy.name, edit_instance_admin_buy_sell_taxonomy_path(taxonomy)
  %td
    .pull-right
      = link_to instance_admin_buy_sell_taxonomy_path(taxonomy), method: :delete,
          data: { confirm: 'Are you sure you want to delete this?' }, title: 'Delete' do
        .fa.fa-trash-o.fa-2x.inline-form-icon

    = link_to edit_instance_admin_buy_sell_taxonomy_path(taxonomy), title: 'Edit' do
      .fa.fa-pencil.fa-2x.inline-form-icon.pull-right
