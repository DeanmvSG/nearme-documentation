- title t('dashboard.nav.waiver_agreement_templates')

.dash-page
  .dash-head.clearfix
    %p= t('dashboard.nav.waiver_agreement_templates')
    = link_to new_dashboard_waiver_agreement_template_path, class: "dash-head-btn", rel: "modal" do
      .btn.blue{:role => "button"} Add Waiver Agreement

  .dash-body
    #waiver-agreement-template-list.table-responsive
      %table.table
        %thead
          %tr
            %th Agreement
            %th Actions
        %tbody
          = render @waiver_agreement_templates
