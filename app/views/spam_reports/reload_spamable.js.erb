  var container = $("[data-<%= @spamable.class.name.underscore.dasherize %>-id=<%= @spamable.id %>]");
  container.html('<%= escape_javascript( render partial: @partial_name, locals: {event: @spamable, comment: @spamable} ) %>')
  <% unless @spam_report.deleted? %>
    container.find("[data-cancel-report]").show()
    container.find("[data-report-spam]").hide()
  <% end %>
