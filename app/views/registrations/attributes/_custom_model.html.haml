- if only_inputs.empty? || f.object.default_profile.try(:instance_profile_type).try(:custom_model_types).try(:map) { |c| "Custom Model - #{c.name}".to_sym }.include?(only_inputs.first)
  - custom_model = f.object.default_profile.instance_profile_type.custom_model_types.find_by(name: only_inputs.first.to_s.split(" - ")[1])
  = f.simple_fields_for :default_profile do |profile_form|
    = render 'registrations/attributes/custom_model_fields', custom_model: custom_model, f: profile_form
