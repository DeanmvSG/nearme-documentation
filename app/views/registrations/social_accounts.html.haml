- title t('dashboard.nav.social_accounts')


.dash-page.verification
  .dash-head.clearfix
    %p= t('dashboard.nav.social_accounts')
  .dash-body.notifications
    - if @supported_providers.any?
      = render "registrations/networks", :supported_providers => @supported_providers
    - else
      %div
        = t('registrations.social_accounts.no_social_providers', marketplace_name: platform_context.name).html_safe
