- if users.empty?
  %p.empty-resultset= t('user_profile.empty_resultset.users')
- else
  / grid
  .grid-a-search-a
    .wrap
      - users.each do |user|
        = render 'shared/person', user: user

  / more
  - if users.next_page
    %p.more-a{:'data-see-more' => true, :'data-see-more-type' => see_more_type}
      = link_to t(:see_more), see_more_route, remote: true
