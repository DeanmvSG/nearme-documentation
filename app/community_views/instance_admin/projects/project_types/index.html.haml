.content-container
  %h4 Manage Project Types
  .row
    .table-container.col-xs-12
      %table.table
        %tbody
          - @project_types.each do |resource|
            %tr
              %td= resource.name.humanize
              %td= link_to 'Edit', edit_instance_admin_projects_project_type_path(resource), class: 'pull-right'

